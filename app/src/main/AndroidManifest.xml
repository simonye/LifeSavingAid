<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jenny.lifesavingaidui">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LifeSavingAidMain"></activity>
        <activity android:name=".SavingTipBank" />
        <activity android:name=".ShowContent" />
        <activity android:name=".UserProfilePage" />
        <activity
            android:name=".EditUserProfile"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".CreateNewEmergency"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".addOption" />
        <activity android:name=".editEmergencyOption" />
        <activity android:name=".getNewInstruction" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register"></activity>
    </application>

</manifest>
